{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  asdf: {
    managerFilePatterns: [
      "/asdf/.tool-versions"
    ]
  },
  packageRules: [
    {
      matchManagers: [
        "asdf"
      ],
      matchUpdateTypes: [
        "patch"
      ],
      enabled: false
    },
    {
      matchManagers: [
        "asdf"
      ],
      matchUpdateTypes: [
        "minor",
        "major"
      ],
      matchCurrentAge: "> 14 days"
    }
  ]
}
